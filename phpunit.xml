<?xml version="1.0" encoding="UTF-8"?>
<phpunit backupGlobals="true" bootstrap="vendor/autoload.php">
    <testsuite name="Users">
        <directory suffix="Test.php">./tests/Users</directory>
    </testsuite>

    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./tests/Users</directory>
        </whitelist>
    </filter>

    <php>
        <var name="somevar" value="hello"></var>
    </php>
</phpunit>
